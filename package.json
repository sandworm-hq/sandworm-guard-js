{
  "name": "sandworm",
  "version": "0.0.2",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/sandworm-hq/sandworm-js.git"
  },
  "scripts": {
    "dev": "webpack --watch --progress",
    "build": "webpack",
    "lint": "yarn eslint ./src/ ./tests/ ./scripts/ ./cli/",
    "test-node-unit": "jest --noStackTrace --config=tests/configs/jest.unit.config.js",
    "test-node-capture": "jest --noStackTrace --config=tests/configs/jest.capture.config.js",
    "test-node-enforce": "jest --noStackTrace --config=tests/configs/jest.enforce.config.js",
    "test-web": "npx playwright test --config=tests/configs/playwright.config.js",
    "test-node-ce": "yarn test-node-enforce && yarn test-node-capture",
    "test-node-all": "yarn test-node-ce && yarn test-node-unit",
    "test-all": "yarn test-node-all && yarn test-web"
  },
  "bin": "cli/index.js",
  "author": "Sandworm <dev@sandworm.dev>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@playwright/test": "^1.23.4",
    "@types/jest": "^28.1.6",
    "babel-loader": "^8.2.5",
    "buffer": "^6.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "jest-junit": "^14.0.0",
    "playwright-watch": "^1.3.23",
    "prettier": "^2.7.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "source-map-js": "^1.0.2"
  }
}
